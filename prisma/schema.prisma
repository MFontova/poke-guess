generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Pokemon {
  id Int @id
  name String
  height Int
  weight Int
  type1 String
  type2 String?
  smallImageUrl String
  largeImageUrl String
  generation Int?
}
